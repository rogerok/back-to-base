    "typecheck": "tsc --noEmit",
    "test:ui": "npx vitest --ui"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.10",
    "@vitest/ui": "4.0.18",
